<h1>Fwitter - Tweets</h1><br>

<h3>Welcome, <%= current_user.username %></h3>
<br><br>

<% Tweet.all.order(id: :desc).each do |tweet| %>
  <div class="row">
    <div class="span12">
      <a href="/users/<%= tweet.user.slug %>"><%= "@" + tweet.user.username %></a>
      <ul>
        <a href="/tweets/<%= tweet.id %>">"<%= tweet.content %>"</a>
      </ul>
    </div>
  </div>
<% end %>